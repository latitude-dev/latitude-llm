{
  "extends": "@latitude-data/typescript-config/base.json",
  "compilerOptions": {
    "outDir": "./public/workers",
    "module": "es2022",
    "noEmit": false,
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "$/*": ["./src/workers/*"],
      "@latitude-data/constants": ["../../packages/constants"],
      "@latitude-data/constants/*": ["../../packages/constants/*"]
    }
  },
  "include": ["./src/workers/**/*.ts"],
  "exclude": ["node_modules"]
}
