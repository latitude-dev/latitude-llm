{
  "name": "@latitude-data/workers",
  "version": "1.0.0",
  "type": "module",
  "description": "Latitude AI Workers for processing logs",
  "private": true,
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "NODE_DEBUG=latitude:debug tsx watch src/server",
    "dev:debug": "tsx watch --inspect-brk src/server",
    "lint": "eslint src/",
    "start": "node -r module-alias/register ./dist --env=production",
    "tc": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org latitude-l5 --project latitude-llm-app ./dist && sentry-cli sourcemaps upload --org latitude-l5 --project latitude-llm-app ./dist"
  },
  "dependencies": {
    "@latitude-data/core": "workspace:^",
    "@latitude-data/env": "workspace:^",
    "@sentry/cli": "^2.37.0",
    "@sentry/node": "^8.30.0",
    "@t3-oss/env-core": "*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@latitude-data/eslint-config": "workspace:^",
    "@latitude-data/typescript-config": "workspace:^",
    "@types/node": "^22.5.1",
    "tsup": "^8.2.4",
    "tsx": "^4.16.2"
  }
}