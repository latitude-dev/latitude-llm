# Copy this file to charts/latitude/values.secrets.yaml and fill the values.
# Do NOT commit the filled file. It is ignored via .gitignore and excluded via .helmignore.

secretEnv:
  # Required
  DATABASE_URL: postgresql://latitude:secret@pg-postgresql.latitude.svc.cluster.local:5432/latitude_production # CHANGE ME
  NEXT_SERVER_ACTIONS_ENCRYPTION_KEY: "<paste-generated-key>" # CHANGE ME (node tools/generate-server-action-key.js)
  DEFAULT_PROVIDER_API_KEY: "sk-..." # Recommended for onboarding

  # Redis (optional)
  CACHE_PASSWORD: ""

  # OAuth / Email (optional)
  GOOGLE_CLIENT_SECRET: ""
  SMTP_HOST: ""
  SMTP_USER: ""
  SMTP_PASS: ""

  # Sentry (optional)
  SENTRY_DSN: ""
  SENTRY_AUTH_TOKEN: ""
  SENTRY_ORG: ""
  SENTRY_PROJECT: ""

  # AWS S3 (optional)
  AWS_REGION: ""
  AWS_ACCESS_KEY: ""
  AWS_ACCESS_SECRET: ""
  PUBLIC_S3_BUCKET: ""
  S3_BUCKET: ""

  # Traefik (unused in k8s; kept for parity, optional)
  TRAEFIK_ACME_EMAIL: ""
  TRAEFIK_ADMIN_PASS: ""

  # PostgreSQL (optional)
  POSTGRES_PASSWORD: ""
