name: Latitude CI
on:
  push:
  pull_request:

jobs:
  test-prompts:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-node@v4
        with:
          node-version: '20'
      - run: npm ci
      - run: npm i @latitude-data/sdk
      - run: node scripts/runPrompt.js
        env:
          LATITUDE_API_KEY: ${{ secrets.LATITUDE_API_KEY }} 
