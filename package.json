{
  "name": "latitude-llm",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "biome lint --diagnostic-level error",
    "buildExamples": "tsx ./examples/scripts/docs/buildDocs.ts",
    "watch:buildExamples": "chokidar 'examples/src/**/*' -c 'pnpm buildExamples'",
    "mintlify_docs": "cd docs && mintlify dev",
    "latitudeDocs": "pnpm buildExamples && concurrently \"pnpm watch:buildExamples\" \"pnpm mintlify_docs\"",
    "tc": "turbo tc",
    "format": "biome format",
    "test": "turbo test",
    "catchup": "pnpm i && pnpm build --filter=\"./packages/**/*\" && pnpm --filter \"@latitude-data/core\" db:migrate",
    "console": "clear && ./bin/console.ts"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.10",
    "@biomejs/biome": "2.1.3",
    "@types/heapdump": "^0.3.4",
    "@types/node": "^22.13.10",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "glob": "^11.0.1",
    "knip": "^5.62.0",
    "mintlify": "4.1.98",
    "recast": "^0.23.11",
    "tsx": "^4.16.2",
    "turbo": "^2.5.5",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": ">=18.x",
        "react-dom": ">=18.x"
      }
    }
  },
  "version": "0.0.1"
}