{
  "name": "latitude-llm",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "./bin/check-eslint-coverage.sh && turbo lint",
    "buildExamples": "tsx ./examples/scripts/docs/buildDocs.ts",
    "watch:buildExamples": "chokidar 'examples/src/**/*' -c 'pnpm buildExamples'",
    "mintlify_docs": "cd docs && mintlify dev",
    "latitudeDocs": "pnpm buildExamples && concurrently \"pnpm watch:buildExamples\" \"pnpm mintlify_docs\"",
    "tc": "turbo tc",
    "prettier": "prettier --write \"{apps,packages}/**/*.{ts,tsx,md}\" --ignore-path .prettierrcignore",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,md}\" --ignore-path .prettierrcignore",
    "test": "turbo test",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && find . -name '.turbo' -type d -prune -exec rm -rf '{}' +",
    "catchup": "pnpm i --ignore-scripts && pnpm build --filter=\"./packages/**/*\" && pnpm --filter \"@latitude-data/web\" workers:build && pnpm --filter \"@latitude-data/core\" db:migrate",
    "console": "clear && ./bin/console.ts",
    "backgroundRun:stressTest": "pnpm --filter @latitude-data/core backgroundRun:stressTest"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.4",
    "@types/heapdump": "^0.3.4",
    "@types/node": "^22.13.10",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "glob": "^11.0.1",
    "mintlify": "4.1.98",
    "prettier": "^3.5.3",
    "recast": "^0.23.11",
    "tsx": "^4.16.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "consolidate": "1.0.4"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": ">=19.x",
        "react-dom": ">=19.x",
        "eslint": ">=9.x"
      }
    }
  },
  "version": "0.0.1"
}
