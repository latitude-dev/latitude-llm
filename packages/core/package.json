{
  "name": "@latitude-data/core",
  "version": "0.0.1",
  "description": "Latitude core contains core services used across the latitude LLM stack",
  "main": "src/index.ts",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./browser": "./src/browser.ts"
  },
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "pnpm run db:generate --custom",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:test": "NODE_ENV=test pnpm run db:migrate",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio --port 3002",
    "lint": "eslint src",
    "tc": "tsc --noEmit",
    "test": "pnpm run db:migrate:test && vitest run --pool=forks",
    "test:watch": "vitest",
    "prettier": "prettier --write src/**/*.ts"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "*",
    "@ai-sdk/azure": "*",
    "@ai-sdk/mistral": "*",
    "@ai-sdk/openai": "*",
    "@latitude-data/compiler": "*",
    "@latitude-data/env": "*",
    "@t3-oss/env-core": "*",
    "ai": "*",
    "bcrypt": "*",
    "drizzle-kit": "*",
    "drizzle-orm": "*",
    "lodash-es": "*",
    "uuid": "*",
    "zod": "*"
  },
  "devDependencies": {
    "@faker-js/faker": "*",
    "@types/bcrypt": "*",
    "@types/lodash-es": "*",
    "@types/node": "*",
    "@types/uuid": "*",
    "@latitude-data/eslint-config": "workspace:*",
    "@latitude-data/typescript-config": "workspace:*",
    "eslint-plugin-drizzle": "^0.2.3",
    "pg-transactional-tests": "^1.0.9",
    "supertest": "^7.0.0",
    "vitest": "^2.0.3"
  }
}
