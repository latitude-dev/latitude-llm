    "@latitude-data/telemetry": "workspace:*",
    "@latitude-data/typescript-config": "workspace:*",
    "@modelcontextprotocol/sdk": "1.13.1",
    "@napi-rs/canvas": "0.1.65",
    "@opentelemetry/api": "catalog:",
