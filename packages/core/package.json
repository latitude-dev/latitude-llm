{
  "name": "@latitude-data/core",
  "version": "0.0.1",
  "description": "Core services used across the latitude LLM stack",
  "type": "module",
  "exports": {
    "./client": {
      "import": "./src/client/index.ts"
    },
    "./constants": {
      "import": "./src/constants.ts"
    },
    "./helpers": {
      "import": "./src/helpers.ts"
    },
    "./plans": {
      "import": "./src/plans.ts"
    },
    "./cache": {
      "import": "./src/cache/index.ts"
    },
    "./data-access": {
      "import": "./src/data-access/index.ts"
    },
    "./data-access/*": {
      "import": "./src/data-access/*.ts"
    },
    "./data-migrations": {
      "import": "./src/data-migrations/index.ts"
    },
    "./events/*": {
      "import": "./src/events/*.ts"
    },
    "./factories": {
      "import": "./src/tests/factories/index.ts"
    },
    "./jobs": {
      "import": "./src/jobs/index.ts"
    },
    "./jobs/constants": {
      "import": "./src/jobs/constants.ts"
    },
    "./jobs/definitions": {
      "import": "./src/jobs/job-definitions/index.ts"
    },
    "./lib/*": {
      "import": "./src/lib/*.ts"
    },
    "./mailers": {
      "import": "./src/mailers/index.ts"
    },
    "./queues": {
      "import": "./src/jobs/queues/index.ts"
    },
    "./queues/types": {
      "import": "./src/jobs/queues/types.ts"
    },
    "./redis": {
      "import": "./src/redis/index.ts"
    },
    "./repositories": {
      "import": "./src/repositories/index.ts"
    },
    "./repositories/*": {
      "import": "./src/repositories/*.ts"
    },
    "./schema/types": {
      "import": "./src/schema/types.ts"
    },
    "./schema/models/*": {
      "import": "./src/schema/models/*.ts"
    },
    "./schema/legacyModels/*": {
      "import": "./src/schema/legacyModels/*.ts"
    },
    "./services/*": {
      "import": "./src/services/*.ts"
    },
    "./test": {
      "import": "./src/tests/useTestDatabase.ts"
    },
    "./websockets/*": {
      "import": "./src/websockets/*.ts"
    },
    "./workers/*": {
      "import": "./src/workers/*.js"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts"
    },
    "./telemetry": {
      "import": "./src/telemetry/index.ts"
    }
  },
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "pnpm run db:generate --custom",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate:test": "NODE_ENV=test pnpm run db:migrate",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio --port 3003",
    "lint": "eslint src",
    "tc": "tsc --noEmit",
    "test": "pnpm run db:migrate:test && vitest run --pool=forks",
    "test:watch": "vitest",
    "backgroundRun:stressTest": "tsx scripts/background-run/stress-test.ts"
  },
