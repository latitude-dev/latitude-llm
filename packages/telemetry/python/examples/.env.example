# ===========================================
# Latitude Configuration (required for all tests)
# ===========================================
LATITUDE_API_KEY=your-latitude-api-key
LATITUDE_PROJECT_ID=123

# ===========================================
# Provider API Keys (set the ones you need)
# ===========================================

# OpenAI (also used by litellm, langchain, llamaindex, haystack, dspy tests)
OPENAI_API_KEY=your-openai-key

# Anthropic
ANTHROPIC_API_KEY=your-anthropic-key

# Groq
GROQ_API_KEY=your-groq-key

# Mistral
MISTRAL_API_KEY=your-mistral-key

# Cohere
COHERE_API_KEY=your-cohere-key

# Together AI
TOGETHER_API_KEY=your-together-key

# Google Gemini
GEMINI_API_KEY=your-gemini-key

# Replicate
REPLICATE_API_TOKEN=your-replicate-token

# Aleph Alpha
ALEPH_ALPHA_API_KEY=your-aleph-alpha-key

# Azure OpenAI
AZURE_OPENAI_API_KEY=your-azure-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com

# AWS (for Bedrock and SageMaker)
AWS_ACCESS_KEY_ID=your-aws-key
AWS_SECRET_ACCESS_KEY=your-aws-secret
AWS_REGION=us-east-1
SAGEMAKER_ENDPOINT_NAME=your-sagemaker-endpoint

# Google Cloud (for Vertex AI)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
GOOGLE_CLOUD_PROJECT=your-gcp-project

# IBM WatsonX
WATSONX_API_KEY=your-watsonx-key
WATSONX_PROJECT_ID=your-watsonx-project-id

# ===========================================
# Local providers (no API key needed)
# ===========================================
# ollama - runs locally
# transformers - runs locally
